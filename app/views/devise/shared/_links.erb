<%- if controller_name != 'sessions' %>
  <div class="redirect"><%= link_to "PŘIHLÁŠENÍ", new_session_path(resource_name) %></div>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div class="redirect"><%= link_to "REGISTRACE", new_registration_path(resource_name) %></div>
<% end -%>
